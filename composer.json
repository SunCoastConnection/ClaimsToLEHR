{
    "name": "suncoast-connection/claims-to-oemr",
    "description": "Convert X12 claims files to OpenEMR database records.",
    "type": "project",
    "authors": [
        {
            "name": "Sam Likins",
            "email": "sam.likins@wsi-services.com",
            "role": "Developer"
        }
    ],
    "require": {
        "symfony/finder": "3.0.*",
        "illuminate/config": "5.2.*",
        "illuminate/database": "5.2.*",
        "illuminate/events": "5.2.*"
   	},
    "autoload": {
        "psr-4": {
            "SunCoastConnection\\ClaimsToOEMR\\": "source/"
        }
    },
    "require-dev": {
        "kicken/gearman-php": "dev-master",
        "phpseclib/phpseclib": "2.0.*",
        "laravel/homestead": "^4",
        "phploc/phploc": "3.0.*",
        "phpunit/phpunit": "5.3.*",
        "mockery/mockery": "0.9.*",
        "mikey179/vfsStream": "1.6.*"
    },
    "autoload-dev": {
        "psr-4": {
            "SunCoastConnection\\ClaimsToOEMR\\Tests\\": "tests/source"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "suggest": {
        "kicken/gearman-php": "Provides API for connecting to a Gearman server as a worker",
        "phpseclib/phpseclib": "Allows remote SSH capabilities to connect to remote server"
    }
}
